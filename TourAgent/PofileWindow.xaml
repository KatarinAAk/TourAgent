<Window x:Class="TourAgent.PofileWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TourAgent"
        mc:Ignorable="d"
        Title="PofileWindow" Height="450" Width="800">
    <Grid>
        <Button Margin="0,0,400,217" Background="White" Name="Profile" Content="Профиль" FontFamily="Rockwell Extra Bold" FontSize="36" Click="Profile_Click"/>
        <Button Margin="0,217,400,0" Background="White" Name="AddTask" Content="Задачи" FontFamily="Rockwell Extra Bold" FontSize="36" Click="AddTask_Click"/>
        <Button Margin="400,0,0,217" Background="White" Name="AddSale" Content="Туры" FontFamily="Rockwell Extra Bold" FontSize="36" Click="AddSale_Click"/>
        <Button Margin="400,217,0,0" Background="White" Name="Report" Content="Отчеты" FontFamily="Rockwell Extra Bold" FontSize="36" Click="Report_Click"/>
    </Grid>
</Window>
